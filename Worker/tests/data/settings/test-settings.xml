<!--<!DOCTYPE preferences SYSTEM "http://java.sun.com/dtd/preferences.dtd">-->
<preferences>
    <root type="user">
        <map/>
        <node name="crawlers">
            <map/>
            <node name="ftp-crawler">
                <map>
                    <entry key="numThreads" value="10"/>
                    <entry key="connection-type" value="passive"/>
                    <entry key="queue" value="queue"/>
                </map>
            </node>
            <node name="smb-crawler">
                <map>
                    <entry key="numThreads" value="10"/>
                    <entry key="queue" value="queue"/>
                </map>
            </node>
        </node>
        <node name="parsers">
            <map/>
            <node name="html-parser">
                <map>
                    <entry key="XHTML" value="true"/>
                    <entry key="ShowWarnings" value="false"/>
                    <entry key="Quiet" value="true"/>
                    <entry key="OutputFileName" value="getResult.xml"/>
                </map>
            </node>
            <node name="xml-parser">
                <map>

                </map>
            </node>
        </node>
    </root>
</preferences>